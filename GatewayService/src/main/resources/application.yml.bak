
spring:
  application:
    name: gateway-service
  cloud:
   gateway:
     routes:
       - id: member-service
         uri: http://localhost:8081
         predicates:
           - Path=/member/**
       - id: paiement-service
         uri: http://localhost:8080
         predicates:
          - path=/payment:8080
   

     default-filters:
      - DedupeResponseHeader=Access-Control-Allow-Origin
     globalcors:
      cors-configurations:
        '[/**]':
          allowedOrigins: "*"
          allowedMethods: "*"
          allowedHeaders: "*"






