


spring:
  application:
    name: getway_service
  cloud:
   gateway:
     routes:
       - id: membre_service
         uri: http://localhost:8081
         predicates:
           - Path=/api/client/**
   

     default-filters:
      - DedupeResponseHeader=Access-Control-Allow-Origin
     globalcors:
      cors-configurations:
        '[/**]':
          allowedOrigins: "*"
          allowedMethods: "*"
          allowedHeaders: "*"






